{
    "resourceSpecId": "33333333-3333-4333-8333-333333333332",
    "stableId": "main-nlb",
    "displayName": "Main Network Load Balancer",
    "category": "network",
    "iconUrl": "https://cdn.sigmoid.us/icons/aws/full-size/svg/networking/elb.svg",
    "resourceType": "loadbalancer.elb",
    "vpcDependency": "required",
    "status": "draft",
    "position": { "x": 200, "y": 200 },
    "terraform": {
        "module": {
            "source": "github.com/sigmoid-hq/terraform-aws-nlb",
            "ref": "main"
        }
    },
    "pricing": {
        "awsPricingLookup": {
            "serviceCode": "AWSELB",
            "unit": "hourly",
            "quantityProperties": ["load_balancer_count", "count"],
            "quantityDefault": 1,
            "filters": [
                { "field": "location", "value": "${location}" },
                { "field": "productFamily", "value": "Load Balancer-Network" },
                { "field": "usagetype", "defaultValue": "LoadBalancerUsage", "required": false }
            ]
        },
        "usageProfiles": {
            "default": "medium",
            "low": {
                "load_balancer_count": 1,
                "count": 1
            },
            "medium": {
                "load_balancer_count": 2,
                "count": 2
            },
            "high": {
                "load_balancer_count": 4,
                "count": 4
            },
            "xhigh": {
                "load_balancer_count": 8,
                "count": 8
            }
        }
    }
}
